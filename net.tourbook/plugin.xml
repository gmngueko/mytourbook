   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">

      <application>
         <run
               class="net.tourbook.application.Application">
         </run>
      </application>

   </extension>


   <extension
         id="product"
         point="org.eclipse.core.runtime.products">

      <product
            application="net.tourbook.application"
            name="MyTourbook">

         <property
               name="windowImages"
               value="icons/application/tourbook16.png,icons/application/tourbook32.png,icons/application/tourbook48.png,icons/application/tourbook64.png,icons/application/tourbook128.png"/>
         <property
               name="aboutText"
               value="%Product_about"/>
         <property
               name="aboutImage"
               value="icons/application/tourbook-about.png"/>
         <property
               name="appName"
               value="MyTourbook">
         </property>
         <property
               name="startupForegroundColor"
               value="BBCBFF">
         </property>
         <property
               name="startupMessageRect"
               value="5,116,390,15">
         </property>
         <property
               name="startupProgressRect"
               value="5,131,390,10">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>

   </extension>


   <extension
         point="org.eclipse.core.runtime.preferences">
